{
  "manifest_version": 2,

  "name": "The Blank App",
  "description": "This is an extension to report visually inaccesible websites",
  "version": "1.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+I",
        "mac": "Command+I"
      }
    }
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
